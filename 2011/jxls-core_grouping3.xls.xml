 </ExcelWorkbook>
 <Styles>
  <Style ss:ID="Default" ss:Name="Normal">
   <Alignment ss:Vertical="Bottom"/>
   <Borders/>
   <Font/>
   <Interior/>
   <NumberFormat/>
   <Protection/>
  </Style>
  <Style ss:ID="s21">
   <Alignment ss:Vertical="Center"/>
   <Borders>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font x:Family="Swiss" ss:Size="12" ss:Color="#FFFFFF" ss:Bold="1"/>
   <Interior ss:Color="#333399" ss:Pattern="Solid"/>
   <Protection/>
  </Style>
  <Style ss:ID="s22">
   <Borders>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font x:Family="Swiss" ss:Size="12" ss:Color="#FFFFFF" ss:Bold="1"/>
   <Interior ss:Color="#333399" ss:Pattern="Solid"/>
   <Protection/>
  </Style>
  <Style ss:ID="s23">
   <Font x:Family="Swiss" ss:Size="12"/>
  </Style>
  <Style ss:ID="s24">
   <NumberFormat ss:Format="Percent"/>
  </Style>
  <Style ss:ID="s25">
   <Font ss:Color="#FFFFFF" ss:Bold="1"/>
   <Interior ss:Color="#008080" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="s26">
   <Alignment ss:Horizontal="Center" ss:Vertical="Bottom"/>
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"
     ss:Color="#FFFFFF"/>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"
     ss:Color="#FFFFFF"/>
    <Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"
     ss:Color="#FFFFFF"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"
     ss:Color="#FFFFFF"/>
   </Borders>
   <Font x:Family="Swiss" ss:Size="8" ss:Bold="1"/>
   <Interior ss:Color="#008080" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="s27">
   <Interior ss:Color="#0000D4" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="s28">
   <Font ss:Color="#FFFFFF" ss:Bold="1"/>
   <Interior ss:Color="#0000D4" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="s29">
   <Font ss:Color="#FFFFFF" ss:Italic="1"/>
   <Interior ss:Color="#0000D4" ss:Pattern="Solid"/>
  </Style>
 </Styles>
 <Worksheet ss:Name="Sheet1">
  <Table ss:ExpandedColumnCount="4" ss:ExpandedRowCount="5" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="12">
   <Column ss:AutoFitWidth="0" ss:Width="117"/>
   <Column ss:AutoFitWidth="0" ss:Width="154"/>
   <Column ss:AutoFitWidth="0" ss:Width="91"/>
   <Column ss:AutoFitWidth="0" ss:Width="131"/>
   <Row ss:AutoFitHeight="0" ss:Height="18" ss:StyleID="s23">
    <Cell ss:StyleID="s21"><Data ss:Type="String">Department//departments</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">${departments.name}//:4</Data></Cell>
    <Cell ss:StyleID="s22"/>
    <Cell ss:StyleID="s22"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s25"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Total</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${departments.staff.name}</Data></Cell>
    <Cell><Data ss:Type="String">${departments.staff.payment}</Data></Cell>
    <Cell ss:StyleID="s24"><Data ss:Type="String">${departments.staff.bonus}</Data></Cell>
    <Cell><Data ss:Type="String">$[B4*(1+C4)]//departments.staff</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s28"><Data ss:Type="String">Totals:</Data></Cell>
    <Cell ss:StyleID="s29"><Data ss:Type="String">$[SUM(B4)]</Data></Cell>
    <Cell ss:StyleID="s27"/>
    <Cell ss:StyleID="s29"><Data ss:Type="String">$[SUM(D4)]</Data></Cell>
   </Row>
  </Table>
  <WorksheetOptions xmlns="urn:schemas-microsoft-com:office:excel">
   <Print>
    <ValidPrinterInfo/>
    <PaperSizeIndex>9</PaperSizeIndex>
    <HorizontalResolution>300</HorizontalResolution>
    <VerticalResolution>300</VerticalResolution>
   </Print>
   <PageLayoutZoom>0</PageLayoutZoom>
   <Selected/>
   <ProtectObjects>False</ProtectObjects>
   <ProtectScenarios>False</ProtectScenarios>
  </WorksheetOptions>
 </Worksheet>
