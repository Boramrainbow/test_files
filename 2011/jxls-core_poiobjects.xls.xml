 <Worksheet ss:Name="Header Footer Set">
  <Table ss:ExpandedColumnCount="5" ss:ExpandedRowCount="17" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="12">
   <Column ss:AutoFitWidth="0" ss:Width="137"/>
   <Column ss:AutoFitWidth="0" ss:Width="156"/>
   <Column ss:AutoFitWidth="0" ss:Width="109"/>
   <Column ss:AutoFitWidth="0" ss:Width="133"/>
   <Row>
    <Cell ss:StyleID="s31"><Data ss:Type="String">&lt;jx:forEach items=&quot;${departments}&quot; var=&quot;department&quot;&gt;</Data></Cell>
   </Row>
   <Row ss:Height="15">
    <Cell ss:StyleID="s23"><Data ss:Type="String">Department</Data></Cell>
    <Cell ss:StyleID="s24"><Data ss:Type="String">${department.name}</Data></Cell>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s25"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Total</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="13" ss:StyleID="s33">
    <Cell ss:StyleID="s34"><Data ss:Type="String">&lt;jx:forEach items=&quot;${department.staff}&quot; var=&quot;employee&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s32"/>
    <Cell ss:StyleID="s32"/>
    <Cell ss:StyleID="s32"/>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${employee.name}</Data></Cell>
    <Cell><Data ss:Type="String">${employee.payment}</Data></Cell>
    <Cell ss:StyleID="s27"><Data ss:Type="String">${employee.bonus}</Data></Cell>
    <Cell><Data ss:Type="String">$[B6*(1+C6)]</Data></Cell>
    <Cell><Data ss:Type="String">${hssfRow.rowNum}</Data></Cell>
   </Row>
   <Row ss:Height="10" ss:StyleID="s35">
    <Cell><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s28"><Data ss:Type="String">Totals:</Data></Cell>
    <Cell ss:StyleID="s29"><Data ss:Type="String">$[SUM(B6)]</Data></Cell>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s29"><Data ss:Type="String">${hssfRow.rowNum}</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s31"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
   </Row>
   <Row ss:Index="11">
    <Cell><Data ss:Type="String">${sheet.getHeader().setLeft('Test Left Header')}</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${sheet.getHeader().setCenter(itDepartment.name)}</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${sheet.getHeader().setRight('Test Right Header')}</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${sheet.getFooter().setCenter('Test Center Footer')}</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${sheet.getFooter().setRight('Test Right Footer')}</Data></Cell>
   </Row>
   <Row ss:Index="17">
    <Cell><Data ss:Type="String">${hssfRow.rowNum}</Data></Cell>
   </Row>
  </Table>
  <WorksheetOptions xmlns="urn:schemas-microsoft-com:office:excel">
   <Print>
    <ValidPrinterInfo/>
    <HorizontalResolution>200</HorizontalResolution>
    <VerticalResolution>200</VerticalResolution>
   </Print>
   <PageLayoutZoom>0</PageLayoutZoom>
   <ProtectObjects>False</ProtectObjects>
   <ProtectScenarios>False</ProtectScenarios>
  </WorksheetOptions>
 </Worksheet>
 <Worksheet ss:Name="Formatted Header Set">
  <Table ss:ExpandedColumnCount="3" ss:ExpandedRowCount="9" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="12">
   <Column ss:AutoFitWidth="0" ss:Width="101"/>
   <Column ss:AutoFitWidth="0" ss:Width="98"/>
   <Row>
    <Cell ss:StyleID="s36"><Data ss:Type="String">Some data</Data></Cell>
    <Cell><Data ss:Type="String">${hssfRow.rowNum}</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${sheet.getHeader().setLeft(sheet.getHeader().font(&quot;Stencil-Normal&quot;, &quot;Italic&quot;) + sheet.getHeader().fontSize(16) + &quot;Stencil-Normal Italic font and size 16&quot;)}</Data></Cell>
   </Row>
   <Row ss:Index="4">
    <Cell ss:StyleID="s25"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Comment</Data></Cell>
   </Row>
   <Row ss:Height="15">
    <Cell ss:StyleID="s34"><Data ss:Type="String">&lt;jx:forEach items=&quot;${employees}&quot; var=&quot;employee&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s32"/>
   </Row>
   <Row ss:Height="24">
    <Cell><Data ss:Type="String">${employee.name}</Data></Cell>
    <Cell ss:StyleID="s37"><Data ss:Type="String">${employee.comment}</Data></Cell>
    <Cell><Data ss:Type="String">${hssfRow.setHeightInPoints(13.5 * size(employee.comment.split(&quot;\r\n&quot;)));}</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s35"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
    <Cell ss:StyleID="s35"/>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${hssfRow.rowNum}</Data></Cell>
   </Row>
  </Table>
  <WorksheetOptions xmlns="urn:schemas-microsoft-com:office:excel">
   <Print>
    <ValidPrinterInfo/>
    <PaperSizeIndex>9</PaperSizeIndex>
    <HorizontalResolution>300</HorizontalResolution>
    <VerticalResolution>300</VerticalResolution>
   </Print>
   <PageLayoutZoom>0</PageLayoutZoom>
   <ProtectObjects>False</ProtectObjects>
   <ProtectScenarios>False</ProtectScenarios>
  </WorksheetOptions>
 </Worksheet>
