 <Worksheet ss:Name="Sheet1">
  <Table ss:ExpandedColumnCount="2" ss:ExpandedRowCount="11" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="12">
   <Column ss:AutoFitWidth="0" ss:Width="102"/>
   <Column ss:AutoFitWidth="0" ss:Width="149"/>
   <Row>
    <Cell ss:StyleID="s21"><Data ss:Type="String">&lt;jx:forEach items=&quot;${departments}&quot; var=&quot;department&quot;&gt;</Data></Cell>
   </Row>
   <Row ss:Height="15">
    <Cell ss:StyleID="s22"><Data ss:Type="String">Department</Data></Cell>
    <Cell ss:StyleID="s23"><Data ss:Type="String">${department.name}</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s24"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s25"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">Payment</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Bonus Total</Data></Cell>
    <Cell ss:StyleID="s27"/>
   </Row>
   <Row ss:Height="15">
    <Cell ss:StyleID="s28"><Data ss:Type="String">&lt;jx:forEach items=&quot;${department.staff}&quot; var=&quot;employee&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s29"/>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${employee.name}</Data></Cell>
    <Cell><Data ss:Type="String">${employee.payment}</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s30"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
    <Cell ss:StyleID="s30"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s31"><Data ss:Type="String">Totals:</Data></Cell>
    <Cell ss:StyleID="s32"><Data ss:Type="String">$[SUM(B7)@0]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s21"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s33"><Data ss:Type="String">Department Totals:</Data></Cell>
    <Cell ss:StyleID="s34"><Data ss:Type="String">$[SUM(B9)]</Data></Cell>
   </Row>
  </Table>
