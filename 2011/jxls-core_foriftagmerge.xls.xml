 <Worksheet ss:Name="Sheet1">
  <Table ss:ExpandedColumnCount="4" ss:ExpandedRowCount="12" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="12">
   <Column ss:AutoFitWidth="0" ss:Width="137"/>
   <Column ss:AutoFitWidth="0" ss:Width="156"/>
   <Column ss:AutoFitWidth="0" ss:Width="109"/>
   <Column ss:AutoFitWidth="0" ss:Width="133"/>
   <Row>
    <Cell ss:StyleID="s29"><Data ss:Type="String">&lt;jx:forEach items=&quot;${departments}&quot; var=&quot;department&quot;&gt;</Data></Cell>
   </Row>
   <Row ss:Height="15">
    <Cell ss:StyleID="s21"><Data ss:Type="String">Department</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">${department.name}</Data></Cell>
    <Cell ss:StyleID="s22"/>
    <Cell ss:StyleID="s22"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s23"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s24"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s24"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s24"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s24"><Data ss:Type="String">Total</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="13" ss:StyleID="s31">
    <Cell ss:StyleID="s32"><Data ss:Type="String">&lt;jx:forEach items=&quot;${department.staff}&quot; var=&quot;employee&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s30"/>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="13" ss:StyleID="s31">
    <Cell ss:StyleID="s32"><Data ss:Type="String">&lt;jx:if test=&quot;${employee.payment &gt; 2000}&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s30"/>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${employee.name}</Data></Cell>
    <Cell><Data ss:Type="String">${employee.payment}</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">${employee.bonus}</Data></Cell>
    <Cell><Data ss:Type="String">$[B#*(1+C#)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:MergeAcross="1" ss:StyleID="s34"><Data ss:Type="String">Total bonus: ${employee.payment * (employee.bonus + 1) }</Data></Cell>
    <Cell ss:StyleID="s25"/>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="13" ss:StyleID="s31">
    <Cell ss:StyleID="s32"><Data ss:Type="String">&lt;/jx:if&gt;</Data></Cell>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s30"/>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="13" ss:StyleID="s31">
    <Cell ss:StyleID="s32"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s30"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">Totals:</Data></Cell>
    <Cell ss:StyleID="s27"/>
    <Cell ss:StyleID="s28"/>
    <Cell ss:StyleID="s27"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s29"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
   </Row>
  </Table>
  <WorksheetOptions xmlns="urn:schemas-microsoft-com:office:excel">
   <Print>
    <ValidPrinterInfo/>
    <HorizontalResolution>200</HorizontalResolution>
    <VerticalResolution>200</VerticalResolution>
    <NumberofCopies>0</NumberofCopies>
   </Print>
   <PageLayoutZoom>0</PageLayoutZoom>
   <Selected/>
   <ProtectObjects>False</ProtectObjects>
   <ProtectScenarios>False</ProtectScenarios>
  </WorksheetOptions>
 </Worksheet>
