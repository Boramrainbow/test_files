 <Worksheet ss:Name="Sheet1">
  <Table ss:ExpandedColumnCount="11" ss:ExpandedRowCount="3" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="12">
   <Column ss:AutoFitWidth="0" ss:Width="100"/>
   <Column ss:AutoFitWidth="0" ss:Width="95"/>
   <Column ss:AutoFitWidth="0" ss:Width="115"/>
   <Column ss:AutoFitWidth="0" ss:Width="91"/>
   <Column ss:AutoFitWidth="0" ss:Width="94"/>
   <Column ss:AutoFitWidth="0" ss:Width="92"/>
   <Column ss:AutoFitWidth="0" ss:Width="72"/>
   <Column ss:AutoFitWidth="0" ss:Width="99"/>
   <Column ss:Index="11" ss:AutoFitWidth="0" ss:Width="103"/>
   <Row ss:Height="15">
    <Cell ss:StyleID="s28"><Data ss:Type="String">Department:</Data></Cell>
    <Cell ss:StyleID="s23"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s23"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s23"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s23"><Data ss:Type="String">Total</Data></Cell>
    <Cell ss:StyleID="s34"><Data ss:Type="String">Department:</Data></Cell>
    <Cell ss:StyleID="s31"/>
    <Cell ss:StyleID="s31"/>
    <Cell ss:StyleID="s31"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s31"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s31"><Data ss:Type="String">Total</Data></Cell>
   </Row>
   <Row ss:Height="15">
    <Cell ss:StyleID="s28"><Data ss:Type="String">${depIT.name}</Data></Cell>
    <Cell ss:StyleID="s29"><Data ss:Type="String">${depIT.staff.name}</Data></Cell>
    <Cell ss:StyleID="s29"><Data ss:Type="String">${depIT.staff.payment}</Data></Cell>
    <Cell ss:StyleID="s30"><Data ss:Type="String">${depIT.staff.bonus}</Data></Cell>
    <Cell ss:StyleID="s29"><Data ss:Type="String">$[C2*(1+D2)]//depIT.staff</Data></Cell>
    <Cell ss:StyleID="s34"><Data ss:Type="String">${depHR.name}</Data></Cell>
    <Cell ss:StyleID="s35"><Data ss:Type="String">Employee://depHR.staff</Data></Cell>
    <Cell ss:StyleID="s32"><Data ss:Type="String">${depHR.staff.name}</Data></Cell>
    <Cell ss:StyleID="s32"><Data ss:Type="String">${depHR.staff.payment}</Data></Cell>
    <Cell ss:StyleID="s33"><Data ss:Type="String">${depHR.staff.bonus}</Data></Cell>
    <Cell ss:StyleID="s32"><Data ss:Type="String">$[I2*(1+J2)]//depHR.staff</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="18">
    <Cell ss:StyleID="s28"/>
    <Cell ss:StyleID="s24"><Data ss:Type="String">Totals:</Data></Cell>
    <Cell ss:StyleID="s27"><Data ss:Type="String">$[SUM(C2)]</Data></Cell>
    <Cell ss:StyleID="s26"/>
    <Cell ss:StyleID="s25"><Data ss:Type="String">$[SUM(E2)]</Data></Cell>
    <Cell ss:StyleID="s34"/>
   </Row>
  </Table>
