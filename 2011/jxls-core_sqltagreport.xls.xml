 <Worksheet ss:Name="Sheet1">
  <Table ss:ExpandedColumnCount="4" ss:ExpandedRowCount="27" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="12">
   <Column ss:AutoFitWidth="0" ss:Width="145"/>
   <Column ss:AutoFitWidth="0" ss:Width="179"/>
   <Column ss:AutoFitWidth="0" ss:Width="99"/>
   <Column ss:AutoFitWidth="0" ss:Width="70"/>
   <Row>
    <Cell ss:StyleID="s21"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Total</Data></Cell>
   </Row>
   <Row ss:Height="15">
    <Cell ss:StyleID="s23"><Data ss:Type="String">&lt;jx:SQL query=&quot;SELECT d.name depname, e.name empname, age, payment, bonus, birthDate FROM employee e, department d where d.id = e.depid order by age desc&quot; var=&quot;employee&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${employee.empname}</Data></Cell>
    <Cell><Data ss:Type="String">${employee.payment}</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">${employee.bonus}</Data></Cell>
    <Cell><Data ss:Type="String">$[B4*(1+C4)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">&lt;/jx:SQL&gt;</Data></Cell>
    <Cell ss:StyleID="s27"/>
    <Cell ss:StyleID="s27"/>
    <Cell ss:StyleID="s27"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s28"><Data ss:Type="String">Totals:</Data></Cell>
    <Cell ss:StyleID="s29"/>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s29"><Data ss:Type="String">$[SUM(D4)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s31"><Data ss:Type="String">SQL grouping test</Data></Cell>
    <Cell ss:StyleID="s32"/>
    <Cell ss:StyleID="s33"/>
    <Cell ss:StyleID="s32"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s23"><Data ss:Type="String">&lt;jx:forEach items=&quot;${rm.exec('SELECT d.name depname, e.name empname, age, payment, bonus, birthDate FROM employee e, department d where d.id = e.depid order by age desc')}&quot; groupBy=&quot;depname&quot;&gt;</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s34"><Data ss:Type="String">Department: ${group.item.depname}</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s21"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Total</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s23"><Data ss:Type="String">&lt;jx:forEach items=&quot;${group.items}&quot; var=&quot;employee&quot;&gt;</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${employee.empname}</Data></Cell>
    <Cell><Data ss:Type="String">${employee.payment}</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">${employee.bonus}</Data></Cell>
    <Cell><Data ss:Type="String">$[B13*(1+C13)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
    <Cell ss:StyleID="s27"/>
    <Cell ss:StyleID="s27"/>
    <Cell ss:StyleID="s27"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s28"><Data ss:Type="String">Totals:</Data></Cell>
    <Cell ss:StyleID="s29"/>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s29"><Data ss:Type="String">$[SUM(D13)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s31"><Data ss:Type="String">SQL subqueries test</Data></Cell>
    <Cell ss:StyleID="s32"/>
    <Cell ss:StyleID="s33"/>
    <Cell ss:StyleID="s32"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s23"><Data ss:Type="String">&lt;jx:forEach items=&quot;${rm.exec('SELECT d.name, d.id FROM department d')}&quot; var=&quot;dep&quot;&gt;</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s34"><Data ss:Type="String">Department: ${dep.name}</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s21"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s22"><Data ss:Type="String">Total</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s23"><Data ss:Type="String">&lt;jx:forEach items=&quot;${rm.exec('SELECT name, age, payment, bonus, birthDate FROM employee e where e.depid = ' + dep.id)}&quot; var=&quot;employee&quot;&gt;</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${employee.name}</Data></Cell>
    <Cell><Data ss:Type="String">${employee.payment}</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">${employee.bonus}</Data></Cell>
    <Cell><Data ss:Type="String">$[B23*(1+C23)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
    <Cell ss:StyleID="s27"/>
    <Cell ss:StyleID="s27"/>
    <Cell ss:StyleID="s27"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s28"><Data ss:Type="String">Totals:</Data></Cell>
    <Cell ss:StyleID="s29"/>
    <Cell ss:StyleID="s30"/>
    <Cell ss:StyleID="s29"><Data ss:Type="String">$[SUM(D23)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s26"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s35"><Data ss:Type="String">Grand Total</Data></Cell>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"><Data ss:Type="String">$[SUM(D25)]</Data></Cell>
